{
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
  },
  "source": {
      "include": ["lib"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
      "plugins/markdown",
      "node_modules/jsdoc-tsimport-plugin/index.js"
  ],
  "markdown": {
      "tags": ["example"]
  },
  "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
  },
  "opts": {
      "destination": "docs",
      "recurse": true,
      "readme": "README.md"
  }
}
